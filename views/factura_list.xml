<record id="view_factura_list" model="ir.ui.view">
    <field name="name">factura.list</field>
    <field name="model">facturas.factura</field>
    <field name="arch" type="xml">
        <tree>
            <field name="numero_factura"/>
            <field name="partner_id"/>
            <field name="fecha_emision"/>
            <field name="fecha_limite_pago"/>
            <field name="estado_factura"/>
        </tree>
    </field>
</record>
